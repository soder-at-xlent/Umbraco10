@inherits UmbracoViewPage<LayoutViewModel>
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@Model.Title</title>

    <link rel="shortcut icon" href="/images/favicon.ico" type="image/x-icon">

    <link href="~/assets/main.css" rel="stylesheet" asp-append-version="true">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
    <body>
        <div class="mobile-menu-canvas">
            <div class="site-container">
                <div class="top-bar-container">
                    @await Html.PartialAsync("_Header", Model)
                </div>
                <div>
                    <div class="main-content-container">
                        <main class="main-content">
                            <div class="container p-sm-0">
                                <div class="row">
                                    <div class="col-md-12 col-lg-9 pr-md-0">
                                        @RenderBody()
                                    </div>
                                    <div class="col-lg-3 d-none d-lg-block d-xl-block right-menu-column">
                                        <div class="sticky-nav">
                                            <div class="nav-card">
                                                <div class="base-card">
                                                </div>
                                                <div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </main>
                    </div>
                </div>
            </div>
        </div>

        @RenderSection("ScriptsBottom", required: false)
    </body>
</html>